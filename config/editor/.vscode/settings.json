// Place your settings in this file to overwrite default and user settings.
{
	"breadcrumbs.enabled": false,
	"cSpell.fixSpellingWithRenameProvider": true,
	"cSpell.advanced.feature.useReferenceProviderWithRename": true,
	"cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
	"cSpell.diagnosticLevel": "Hint",
	"cSpell.language": "en",
	"C_Cpp.autocompleteAddParentheses": true,
	// Debug settings
	"debug.javascript.codelens.npmScripts": "never",
	"debug.onTaskErrors": "debugAnyway",
	"debug.allowBreakpointsEverywhere": true,
        "debug.inlineValues": "auto",
	// Diff Editor settinsg
	"diffEditor.wordWrap": "on",
	"diffEditor.maxComputationTime": 0,
	"diffEditor.renderSideBySide": false,
	// Editor settings
	"editor.fontSize": 19.2,
	"editor.fontWeight": "normal",
	"editor.wordWrap": "on",
	"editor.bracketPairColorization.enabled": true,
	"editor.fontFamily": "Fira Code",
	"editor.fontLigatures": true,
	"editor.detectIndentation": false,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.snippetSuggestions": "top",
	"editor.unicodeHighlight.allowedCharacters": {
		"â€™": true
	},
	"editor.suggestSelection": "first",
	"editor.autoClosingQuotes": "always",
	"editor.autoClosingBrackets": "always",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"editor.inlineSuggest.enabled": true,
	"editor.minimap.enabled": false,
	"editor.tabSize": 4,
	"editor.renderWhitespace": "none",
	"editor.cursorBlinking": "smooth",
	"editor.cursorWidth": 2,
	"editor.rulers": [
		180
	],
	"editor.lineHeight": 24,
	"editor.wordWrapColumn": 190,
	"editor.overviewRulerBorder": false,
	"editor.linkedEditing": true,
	"editor.formatOnPaste": false,
	"editor.smoothScrolling": true,
	"editor.tokenColorCustomizations": {
		"comments": "#dddddd69"
	},
	"editor.autoIndent": "full",
	"editor.colorDecorators": true,
	"editor.trimAutoWhitespace": true,
	"editor.scrollBeyondLastLine": false,
	"editor.renderControlCharacters": true,
	// Explorer settings
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	// Files settings
	"files.autoSave": "off",
	"files.autoSaveDelay": 5000,
	"files.trimFinalNewlines": true,
	"files.fileAssociations": {
		".c": "c",
		".h": "c",
		".jsx": "javascriptreact",
		"*.html": "html"
	},
	"editor.fileAssociations": {
		"*.c": "C",
		"*.h": "C",
		"*.html": "html",
		"*.php": "php"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/node_modules": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/.next": true
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.maxTokenizationLineLength": 2500
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml",
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "keep",
		"editor.quickSuggestions": {
			"other": true,
			"comments": false,
			"strings": true
		}
	},
	"[xml]": {
		"editor.defaultFormatter": "redhat.vscode-xml"
	},
	"[css]": {
		"editor.defaultFormatter": "aeschli.vscode-css-formatter"
	},
	"[vb]": {},
	"[c]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"[scss]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"[sql]": {
		"editor.defaultFormatter": ""
	},
	"[csharp]": {
		"editor.defaultFormatter": "ms-dotnettools.csharp"
	},
	"[cpp]": {
		"editor.defaultFormatter": "ms-vscode.cpptools",
		"editor.formatOnSave": true
	},
	"[markdown]": {
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.unicodeHighlight.invisibleCharacters": false,
		"editor.wordWrap": "on",
		"editor.quickSuggestions": {
			"comments": "off",
			"strings": "off",
			"other": "off"
		},
		"cSpell.fixSpellingWithRenameProvider": true,
		"cSpell.advanced.feature.useReferenceProviderWithRename": true,
		"cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
		"editor.defaultFormatter": "vscode.markdown-language-features"
	},
	"[php]": {
		"cSpell.fixSpellingWithRenameProvider": false,
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	// PHP settings
	"php.suggest.basic": true,
	"php.validate.executablePath": "",
	// Python settings 
	"python.linting.pylintEnabled": true,
	"python.linting.enabled": true,
	"python.defaultInterpreterPath": "/usr/bin/python3",
	"python.languageServer": "Pylance",
	// Git settings 
	"git.autofetch": true,
	"git.ignoreLegacyWarning": true,
	"git.suggestSmartCommit": false,
	"git.confirmSync": false,
	"git.allowForcePush": true,
	"git.terminalAuthentication": false,
	"git.fetchOnPull": true,
	"window.zoomLevel": 0,
	"window.autoDetectColorScheme": true,
	"window.menuBarVisibility": "classic",
	// Workbench settings
	"workbench.colorTheme": "GitHub Dark Default",
	"workbench.activityBar.visible": false,
	"workbench.iconTheme": "vs-seti",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook",
		"*.bin": "default",
		"*.html": "default",
		"*.odt": "default",
		"*.docx": "default",
		"*.c": "default",
		"*.h": "default"
	},
	"workbench.sideBar.location": "left",
	"workbench.startupEditor": "newUntitledFile",
	"workbench.preferredLightColorTheme": "GitHub Light Default",
	"workbench.preferredDarkColorTheme": "GitHub Dark Default",
	// Emmet settings

	"emmet.includeLanguages": {
		"javascript": "javascript",
		"html": "html",
	},
	
	"emmet.showExpandedAbbreviation": "always",
	"emmet.triggerExpansionOnTab": true,
	// Eslint settings
	"eslint.enable": true,
	"eslint.options": {
		"parserOptions": {
			"ecmaVersion": "",
			"sourceType": "script",
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"env": {
			"browser": true,
			"es6": true
		},
		"root": true,
		"extends": [
			""
		],
		"overrides": [],
		"files": [
			"*.jsx",
			"*.js"
		],
		"overrideConfigFile": ".vscode/.eslintrc.js"
	},
	"eslint.nodePath": "/usr/local/lib/node_modules/",
	"eslint.format.enable": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"html",
		"json",
		"jsonc"
	],
	"eslint.useESLintClass": true,
	"eslint.alwaysShowStatus": true,
	"eslint.codeAction.disableRuleComment": {
		"enable": true,
		"location": "separateLine"
	},
	"eslint.codeAction.showDocumentation": {
		"enable": true
	},
	"eslint.lintTask.enable": true,
	"eslint.run": "onType",
	"eslint.packageManager": "npm",
	"html.suggest.html5": true,
	"html.autoClosingTags": false,
	"html.validate.scripts": true,
	"html.validate.styles": true,
	"javascript.format.enable": true,
	"prettier.configPath": ".prettierrc",
	"prettier.requireConfig": true,
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"prettier.printWidth": 100,
	"prettier.semi": false,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.tabWidth": 4,
	"prettier.eslintIntegration": true,
        "prettier.bracketSameLine": false,
        "prettier.bracketSpacing": true,
	"terminal.integrated.defaultProfile.linux": "bash",
	"terminal.integrated.profiles.linux": {
		"bash": {
			"path": "bash",
			"icon": "terminal-bash"
		},
		"zsh": {
			"path": "zsh"
		},
		"fish": {
			"path": "fish"
		},
		"tmux": {
			"path": "tmux",
			"icon": "terminal-tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		}
	},
	"liveServer.settings.AdvanceCustomBrowserCmdLine": "",
	"liveServer.settings.ChromeDebuggingAttachment": true,
	"liveServer.settings.host": "localhost",
	"liveServer.settings.donotVerifyTags": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.port": 5504,
	"liveServer.settings.CustomBrowser": "firefox",
	"liveServer.settings.useLocalIp": true,
	"liveServer.settings.multiRootWorkspaceName": "",
	"liveServer.settings.ignoreFiles": [
		"**"
	],
	"liveshare.allowInvites": "all",
	"liveshare.guestApprovalRequired": false,
	"liveshare.anonymousGuestApproval": "reject",
	"liveshare.presence": true,
	"githubPullRequests.queries": [
		{
			"label": "Waiting For My Review",
			"query": "is:open review-requested:${user}"
		},
		{
			"label": "Assigned To Me",
			"query": "is:open assignee:${user}"
		},
		{
			"label": "Created By Me",
			"query": "is:open author:${user}"
		}
	],
	"githubIssues.queries": [
		{
			"label": "My Issues",
			"query": "default"
		},
		{
			"label": "Created Issues",
			"query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
		}
	],
	"githubPullRequests.remotes": [
		"origin",
		"upstream",
		"master",
		"main"
	],
	"cSpell.userWords": [],
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"cmake.configureOnOpen": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"mssql.connections": [
		{
			"server": "{{put-server-name-here}}",
			"database": "{{put-database-name-here}}",
			"user": "{{put-username-here}}",
			"password": ""
		}
	],
	"redhat.telemetry.enabled": true,
	"codeQL.telemetry.enableTelemetry": false,
	"editor.semanticHighlighting.enabled": true,
	"editor.semanticTokenColorCustomizations": {},
	"files.associations": {
		".c": "C",
		".h": "C",
		"*.html": "html",
		"*.php": "php"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"settingsSync.ignoredSettings": [],
	"intelephense.phpdoc.classTemplate": {
		"summary": "$1",
		"tags": [
			"@package ${1:$SYMBOL_NAMESPACE}"
		]
	},
	"xml.fileAssociations": [],
	"editor.guides.indentation": false,
	"workbench.editor.enablePreview": false
}
